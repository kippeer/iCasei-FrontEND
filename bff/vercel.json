{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/youtube/(.*)",
        "dest": "bff/src/routes/youtubeRoutes.js"
      },
      {
        "src": "/api/favoritos/(.*)",
        "dest": "bff/src/routes/favoritosRoutes.js"
      }
    ]
  }
  